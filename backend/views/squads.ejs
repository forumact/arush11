<div class="container-fluid">
  <form action="/playing-11">
    <div class="row">
      <div class="col-md-6 mt-3 mb-3">
        <% teamrole1.forEach(function(teams, inc){ %>
        <section class="team-bg">
          <div class="d-flex justify-content-between bg-transparent">
            <div class="p-2">
              <%- include('functions'); %>
              <div class="d-inline-block team-name-1 f13 fw-600">
                <%= team1.toUpperCase() %>
              </div>
            </div>
            <div class="text-white mt-2 f13 fw-600">Squads - <img src="<%= team1img %>" class="flag-css" /></div>
            <div class="p-2">
              <%- include('functions'); %>
              <div class="d-inline-block team-name-1 f13 fw-600">Credits</div>
              <span class="text-bold text-white f13 fw-600"> <%= credits(teams)%> </span>
            </div>
          </div>
          <div class="d-flex justify-content-center">
            <% for (const teamswk of teams['WK'] || []) { %> <%-
            include('partials/playerpreview', {teams: teamswk, inc:inc}); %> <%
            } %>
          </div>
          <div class="d-flex justify-content-center">
            <% for (const teamsbat of teams['BAT'] || []) { %> <%-
            include('partials/playerpreview', {teams: teamsbat, inc:inc}); %> <%
            } %>
          </div>
          <div class="d-flex justify-content-center">
            <% for (const teamsar of teams['AR'] || []) { %> <%-
            include('partials/playerpreview', {teams: teamsar, inc:inc}); %> <%
            } %>
          </div>
          <div class="d-flex justify-content-center">
            <% for (const teamsbowl of teams['BOWL'] || []) { %> <%-
            include('partials/playerpreview', {teams: teamsbowl, inc:inc}); %>
            <% } %>
          </div>
        </section>
        <% }); %>
        <input type="hidden" name="team1" class="team1" value="<%= team1 %>" />
      </div>
      <div class="col-md-6 mt-3 mb-3">
        <% teamrole2.forEach(function(teams, inc){ %>
        <section class="team-bg">
          <div class="d-flex justify-content-between bg-transparent">
            <div class="p-2">
              <%- include('functions'); %>
              <div class="d-inline-block team-name-2 f13 fw-600">
                <%= team2.toUpperCase() %>
              </div>
            </div>
            <div class="text-white mt-2 f13 fw-600">Squads - <img src="<%= team2img %>" class="flag-css" /></div>
            <div class="p-2">
              <%- include('functions'); %>
              <div class="d-inline-block team-name-2 f13 fw-600">Credits</div>
              <span class="text-bold text-white f13 fw-600"> <%= credits(teams)%> </span>
            </div>
          </div>
          <div class="d-flex justify-content-center">
            <% for (const teamswk of teams['WK'] || []) { %> <%-
            include('partials/playerpreview', {teams: teamswk, inc:inc}); %> <%
            } %>
          </div>
          <div class="d-flex justify-content-center">
            <% for (const teamsbat of teams['BAT'] || []) { %> <%-
            include('partials/playerpreview', {teams: teamsbat, inc:inc}); %> <%
            } %>
          </div>
          <div class="d-flex justify-content-center">
            <% for (const teamsar of teams['AR'] || []) { %> <%-
            include('partials/playerpreview', {teams: teamsar, inc:inc}); %> <%
            } %>
          </div>
          <div class="d-flex justify-content-center">
            <% for (const teamsbowl of teams['BOWL'] || []) { %> <%-
            include('partials/playerpreview', {teams: teamsbowl, inc:inc}); %>
            <% } %>
          </div>
        </section>
        <% }); %>
        <input type="hidden" name="team2" class="team2" value="<%= team2 %>" />
      </div>
      <div class="bg-dark p-3 d-flex justify-content-center">
        <input type="hidden" name="destUrl" value="<%= destUrl %>" />
        <input type="hidden" name="matchid" value="<%= matchid %>" />
        <button
          type="submit"
          class="btn btn-warning float-center text-dark teampartition"
        >
          Next
        </button>
      </div>
    </div>
  </form>
</div>
