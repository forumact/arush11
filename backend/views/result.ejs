    <div class="w3-panel success">
      <div class="alert alert-success alert-dismissible fade show" role="alert">
        <%- multiteams.length %> Teams Listed
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
      </div>
    </div>
<div class="container-fluid">
  <div class="row justify-content-center">
    <% multiteams.forEach(function(teams, inc){ %>
    <div class="col-auto mt-4">
      <section class="team-bg">
        <div class="d-flex justify-content-between align-items-start bg-transparent">
          <div class="p-2">
            <%- include('functions'); %>
            <div class="d-inline-block team-name-1 f13 fw-600"><%= team1.toUpperCase() %></div>
            <span class="text-bold text-white f13 fw-600">
              <%= teamPlayercount(teams, team1) %>:<%= teamPlayercount(teams, team2) %>
            </span>
            <div class="d-inline-block team-name-2 f13 fw-600"><%= team2.toUpperCase() %></div>
          </div>
          <div class="text-white f13 fw-600 align-self-center bg-light p-0-5">T-<%- teams['WK'][0].teamnumber %> </div>
          <div class="p-2">
            <%- include('functions'); %>
            <div class="d-inline-block team-name-2 f13 fw-600">Cr</div>
            <span class="text-bold text-white f13 fw-600"><%= credits(teams)%></span>
            <div class="d-inline-block team-name-1 f13 fw-600">Pts</div>
            <span class="text-bold text-white f13 fw-600"><%= points(teams)%></span>
          </div>
        </div>
        <div class="d-flex justify-content-center">
          <% for (const teamswk of teams['WK']) { %>
           <%- include('partials/resultplayer', {teams: teamswk, inc:inc}); %>
          <% } %>
        </div>
        <div class="d-flex justify-content-center">
          <% for (const teamsbat of teams['BAT']) { %>
             <%- include('partials/resultplayer', {teams: teamsbat, inc:inc}); %>
          <% } %>
        </div>
        <div class="d-flex justify-content-center">
          <% for (const teamsar of teams['AR']) { %>
            <%- include('partials/resultplayer', {teams: teamsar, inc:inc}); %>
          <% } %>
        </div>
        <div class="d-flex justify-content-center">
          <% for (const teamsbowl of teams['BOWL']) { %>
            <%- include('partials/resultplayer', {teams: teamsbowl, inc:inc}); %>
          <% } %>
        </div>
      </section>
    </div>
    <% }); %>
  </div>
</div>
